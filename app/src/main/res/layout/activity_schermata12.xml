<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".schermata_12">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".schermata12">

        <Button
            android:id="@+id/BottoneX"
            android:layout_width="39dp"
            android:layout_height="31dp"
            android:backgroundTint="#00000000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.15"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.102" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/schermata_12"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp" />

        <Button
            android:id="@+id/BottoneInvia12"
            android:layout_width="164dp"
            android:layout_height="52dp"
            android:backgroundTint="#00000000"
            app:layout_constraintBottom_toBottomOf="@+id/imageView12"
            app:layout_constraintEnd_toEndOf="@+id/imageView12"
            app:layout_constraintHorizontal_bias="0.922"
            app:layout_constraintStart_toStartOf="@+id/imageView12"
            app:layout_constraintTop_toTopOf="@+id/imageView12"
            app:layout_constraintVertical_bias="0.763" />


        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/TextInputPassword"
            android:layout_width="274dp"
            android:layout_height="40dp"
            android:background="@drawable/style"
            android:hint="@string/NuovaPassword"
            android:shadowColor="#8A8A8A"
            android:textColor="#8A8A8A"
            android:textColorHighlight="#8A8A8A"
            android:textColorHint="#8A8A8A"
            android:textColorLink="#8A8A8A"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView12"
            app:layout_constraintEnd_toEndOf="@+id/imageView12"
            app:layout_constraintHorizontal_bias="0.885"
            app:layout_constraintStart_toStartOf="@+id/imageView12"
            app:layout_constraintTop_toTopOf="@+id/imageView12"
            app:layout_constraintVertical_bias="0.347" />

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/TextInputConfermaPassword"
            android:layout_width="274dp"
            android:layout_height="40dp"
            android:background="@drawable/style"
            android:hint="@string/ConfermaPassword"
            android:shadowColor="#8A8A8A"
            android:textColor="#8A8A8A"
            android:textColorHighlight="#8A8A8A"
            android:textColorHint="#8A8A8A"
            android:textColorLink="#8A8A8A"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView12"
            app:layout_constraintEnd_toEndOf="@+id/imageView12"
            app:layout_constraintHorizontal_bias="0.885"
            app:layout_constraintStart_toStartOf="@+id/imageView12"
            app:layout_constraintTop_toTopOf="@+id/imageView12"
            app:layout_constraintVertical_bias="0.533" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/BottoneAnnulla12"
        android:layout_width="163dp"
        android:layout_height="53dp"
        android:backgroundTint="#00000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.688"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.763" />

</androidx.constraintlayout.widget.ConstraintLayout>